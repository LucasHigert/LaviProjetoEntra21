
@{
    Layout = null;
}

<div class="modal" tabindex="-1" role="dialog" id="encaminhamento-posto-modal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cadastrar Encaminhamento</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

             
                <div class="form-group">
                    <label for="modal-produto-valor">Valor</label>
                    <input id="modal-encaminhamento-x"
                           class="form-control" />
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-danger" data-dismiss="modal">Fechar</button>
                <button type="button" class="btn btn-success"
                        id="modal-encaminhamento-salvar">
                    Salvar
                </button>
            </div>
        </div>
    </div>
</div>
