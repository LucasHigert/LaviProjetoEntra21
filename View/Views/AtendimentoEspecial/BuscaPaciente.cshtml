
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Atendimento estrangeiro</title>
    <link rel="shortcut icon" href="~/Content/Imagens/Logo Lavi Preto.png" />
    <link rel="stylesheet" href="~/Scripts/Tema/global/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Scripts/Tema/global/fonts/font-awesome/font-awesome.min.css" />
</head>
<body>
    <div class="bg-dark d-flex p-3 px-md-4 mb-3">
        <div class="col-12 text-center">
            <img src="~/Content/Imagens/Logo Lavi.png" width="50" height="50" />

        </div>
    </div>
    <div class="container">
        <h1 class="h1">@View.Resources.Resource.VocêPossuiCadastro</h1>
        <div>
            <label for="campo-nome" class="font-weight-bold">@View.Resources.Resource.DigiteSeuNomePesquisar</label>
            <input id="campo-nome" name="nome" class="form-control border-info" type="text" />

        </div>
        <div>
            <table class="table table-hover table-info table-bordered mt-20">
                <thead class="bg-dark text-white">
                    <tr>
                        <td>@View.Resources.Resource.Nome</td>
                        <td width="5    0px">@View.Resources.Resource.Acao</td>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Wallace</td>
                        <td>
                            <a class="btn btn-primary btn-sm text-white">@View.Resources.Resource.EsteSouEu</a>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>



        <a href="/atendimentoespecial/cadastropacienteespecial" class="btn btn-primary text-white">@View.Resources.Resource.NaoTenho</a>
    </div>

    <script src="~/Scripts/Tema/global/vendor/bootstrap/bootstrap.min.js"></script>
    <script href="~/Scripts/Tema/global/vendor/jquery/jquery.min.js"></script>
</body>
</html>
