<div id="modal-sintoma" class="modal">
    <div class="modal-dialog">
        <!--Titulo da modal-->
        <div class="modal-content">
            <h2>@View.Resources.Resource.Sintoma</h2>
            <button type="button" class="close" data-dismiss="modal">
                <span>&times;</span>
            </button>
        </div>

        <div class="modal-body">

        </div>

        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">@View.Resources.Resource.Cancelar</button>
            <button type="button" class="btn btn-primary" id="pessoa-botao-salvar">@View.Resources.Resource.Proximo</button>
        </div>

    </div>

</div>
