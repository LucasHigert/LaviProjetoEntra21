
@{
    ViewBag.Title = "Index";
}

<h2>Cargos</h2>
<table class="table table-striped table-bordered table-hover" id="cidades-index">
    <thead>
        <tr>
            <th>Cargos</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Model.Cargo cargo in ViewBag.Cargos)
        {
            <tr>
              <td>@cargo.Nome</td>
          </tr>
        }
    </tbody>
</table>